#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/citi/node_modules/.pnpm/fast-xml-parser@4.0.11/node_modules/fast-xml-parser/src/cli/cli.js/node_modules:/home/workspace/citi/node_modules/.pnpm/fast-xml-parser@4.0.11/node_modules/fast-xml-parser/src/cli/node_modules:/home/workspace/citi/node_modules/.pnpm/fast-xml-parser@4.0.11/node_modules/fast-xml-parser/src/node_modules:/home/workspace/citi/node_modules/.pnpm/fast-xml-parser@4.0.11/node_modules/fast-xml-parser/node_modules:/home/workspace/citi/node_modules/.pnpm/fast-xml-parser@4.0.11/node_modules:/home/workspace/citi/node_modules/.pnpm/node_modules:/home/workspace/citi/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/citi/node_modules/fast-xml-parser/src/cli/cli.js/node_modules:/home/workspace/citi/node_modules/fast-xml-parser/src/cli/node_modules:/home/workspace/citi/node_modules/fast-xml-parser/src/node_modules:/home/workspace/citi/node_modules/fast-xml-parser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fast-xml-parser/src/cli/cli.js" "$@"
else 
  exec node  "$basedir/../fast-xml-parser/src/cli/cli.js" "$@"
fi
